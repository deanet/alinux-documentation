<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>alinux.web.id feed</title>
	<link type="application/atom+xml" href="{{ site.url }}/atom.xml" rel="self" />
	<link type="text/html" href="{{ site.url }}/" rel="alternate" />
	<id>tag:alinux.web.id,2010-05-24:/atom.xml</id>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	{% for post in site.posts limit:10 %}
	<entry>
		<title>{{ post.title }}</title>
		<author>
			<name>alinux.web.id</name>
		</author>
		<link type="text/html" href="{{ site.url }}{{ post.url }}" rel="alternate" />
		<updated>{{ post.date | date: "%Y-%m-%dT%H:%M:%S-07:00" }}</updated>
		<id>tag:alinux.web.id,{{ post.date | date: "%Y-%m-%d" }}:{{ post.id }}</id>
		<content type="html">
		{{ post.content | xml_escape | truncatewords: 20 }} [..]

	</content>
	</entry>{% endfor %}
</feed>
